{
  "manifest_version": 2,

  "name": "Numbered Tabs",
  "description": "This extension will add numbers to tab titles allowing for quicker navigation",
  "version": "1.0",

  "browser_action": {
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
   "tabs",
   "http://*/",
   "https://*/"
  ]
}
