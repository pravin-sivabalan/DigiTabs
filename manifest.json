{
  "manifest_version": 2,

  "name": "Numbered Tabs",
  "description": "This extension will add numbers to tab titles allowing for quicker navigation",
  "version": "1.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
   "tabs",
   "http://*/",
   "https://*/"
  ]
}
